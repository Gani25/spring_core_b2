	public SPRKDealership(CarBrand carBrand) {
		System.out.println("Constructor Injection -> Finalizing Dealership with "+carBrand.getClass().getSimpleName()+" Cars");
		this.carBrand = carBrand;
	}

	public CarBrand getCarBrand() {
		return carBrand;
	}

	public void setCarBrand(CarBrand carBrand) {
		System.out.println(" Setter Injection -> Finalizing Dealership with "+carBrand.getClass().getSimpleName()+" Cars.");
		this.carBrand = carBrand;
	}
	
	

