package com.sprk.main;

import org.springframework.context.ApplicationContext;
import org.springframework.context.annotation.AnnotationConfigApplicationContext;

import com.sprk.configurations.DemoConfig;
import com.sprk.showroom.SPRKDealership;

public class ClassBasedInjection {
	public static void main(String[] args) {
		
		
		ApplicationContext context = new AnnotationConfigApplicationContext(DemoConfig.class);
		System.out.println("Hello");
		SPRKDealership dealership = context.getBean("dealershipBean",SPRKDealership.class)
	}

}
